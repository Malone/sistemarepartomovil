<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adis.srm.sistemarepartomovil">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.DispatchActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="DATABASE"
            android:value="srm029.db" />
        <meta-data
            android:name="VERSION"
            android:value="25" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.adis.srm.sistemarepartomovil.entity" />

        <activity
            android:name=".activity.InvoiceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.EditInvoiceItemActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.EntregarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.NoEntregarActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>